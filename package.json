{
  "name": "capacitor-firebase-auth",
  "version": "4.0.0",
  "description": "Capacitor plugin for Firebase Authentication",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "pack": "npm pack && mv capacitor-firebase-auth-3.*.tgz capacitor-firebase-auth-nightly.tgz",
    "watch": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "wyattades",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^3.2.3",
    "@capacitor/core": "^3.2.3",
    "@capacitor/ios": "^3.2.3",
    "firebase": "^8.9.1",
    "prettier": "2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@capacitor/core": "^3.2.3",
    "firebase": "^8.9.1"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorFirebaseAuth.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wyattades/capacitor-firebase-auth.git"
  },
  "bugs": {
    "url": "https://github.com/wyattades/capacitor-firebase-auth.git/issues"
  }
}
